<h1>Listing Employee Events</h1>

<table>
  <thead>
    <tr>
      <th>Kind</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emp_transactions.each do |emp_transaction| %>
      <tr>
        <td><%= emp_transaction.kind %></td>
        <td><%= emp_transaction.user_id %></td>
        <td><%= link_to 'Show', emp_transaction %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
